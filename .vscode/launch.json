{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "CS2 Callouts: Extract de_mirage",
            "type": "python",
            "request": "launch",
            "module": "cs2_callouts",
            "args": ["extract", "--map", "de_mirage"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {},
            "justMyCode": false
